<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssg.membertest.member.mapper.MemberMapper">

    <insert id="insertMember" parameterType="com.ssg.membertest.member.MemberDTO">
        insert into t_member(mid, mpw, mname)
        values(#{mid},#{mpw},#{mname})
    </insert>

    <select id="selectMembers" resultType="com.ssg.membertest.member.MemberDTO">
        select * from t_member
    </select>

</mapper>